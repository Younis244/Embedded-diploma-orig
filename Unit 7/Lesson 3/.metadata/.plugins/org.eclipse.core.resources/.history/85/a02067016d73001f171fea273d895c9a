#ifndef INC_STM32_F103C6_GPIO_DRIVERS_H_
#define INC_STM32_F103C6_GPIO_DRIVERS_H_

//-----------------------------
//Includes
//-----------------------------

#include "stm32f103x6.h"

//==============================================================//


//-----------------------------
//Config structure
//-----------------------------
typedef struct
{
	/*Specifies the gpio pins to be configured
	 *This paraeter can be a value of @ref GPIO_Pins_define*/
	uint16_t GPIO_Pin_Num;



}GPIO_Pin_Config_t;//t->typedef


//-----------------------------
//Macros Configuration Refrences
//-----------------------------

// @ref GPIO_Pins_define
#define GPIO_Pin_0						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_1						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_2						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_3						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_4						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_5						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_6						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_7						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_8						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_9						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_10						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_11						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_12						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_13						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_14						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_15						((uint16_t)0x0001) /*Pin 0 selected*/
#define GPIO_Pin_All						((uint16_t)0x0001) /*Pin 0 selected*/


#endif /* INC_STM32_F103C6_GPIO_DRIVERS_H_ */
